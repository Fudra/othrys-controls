mixin filter(parent, name)
    .row.margin-top-20
        .col.s6.max-height-120
            +knob(parent + '-' + name + '-' + 'gain', 'Gain', 25,'#ffec03')
        .col.s6.max-height-120
            +knob(parent + '-' + name + '-' + 'frequency', 'Frequency', 500, '#66CC66')
        .col.s6.max-height-120
            +knob(parent + '-' + name + '-' + 'detune', 'Detune', 10, '#C0ffff')
        //.col.s6.max-height-120
        //    +knob(parent +  '-' + name +'' +'-q', 'Q','#C0ffff')
